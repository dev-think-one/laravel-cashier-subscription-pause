{
  "name": "think.studio/laravel-cashier-subscription-pause",
  "description": "Support of Pausing and Resuming a Subscription.",
  "keywords": [
    "laravel",
    "cashier",
    "subscription",
    "pause"
  ],
  "homepage": "https://github.com/dev-think-one/laravel-cashier-subscription-pause",
  "license": "MIT",
  "authors": [
    {
      "name": "Yaroslav Georgitsa",
      "email": "yg@think.studio",
      "homepage": "https://github.com/yaroslawww",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "laravel/cashier": "^14.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.16",
    "orchestra/testbench": "^8.5",
    "phpunit/phpunit": "^10.1",
    "psalm/plugin-laravel": "^2.8",
    "vimeo/psalm": "^5.11"
  },
  "autoload": {
    "psr-4": {
      "CashierSubscriptionPause\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CashierSubscriptionPause\\Tests\\": "tests"
    }
  },
  "scripts": {
    "psalm": "vendor/bin/psalm",
    "test": "XDEBUG_MODE=coverage vendor/bin/phpunit --colors=always",
    "test-coverage": "XDEBUG_MODE=coverage vendor/bin/phpunit --colors=always --coverage-html coverage",
    "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "CashierSubscriptionPause\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
